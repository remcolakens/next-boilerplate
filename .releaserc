{
	"branches": [
		"main",
		{
			"name": "beta",
			"channel": "beta",
			"prerelease": true
		}
	],
	"plugins": [
		[
			"@semantic-release/commit-analyzer",
			{
				"preset": "angular",
				"releaseRules": [
					{
						"type": "refactor",
						"release": "patch"
					},
					{
						"type": "chore",
						"release": "patch"
					},
					{
						"type": "docs",
						"release": "patch"
					}
				]
			}
		],
		[
			"@semantic-release/release-notes-generator",
			{
				"preset": "conventionalcommits",
				"presetConfig": {
					"types": [
						{
							"type": "feat",
							"section": "ğŸš€ Features"
						},
						{
							"type": "perf",
							"section": "ğŸ Performance"
						},
						{
							"type": "fix",
							"section": "ğŸ Bug Fixes"
						},
						{
							"type": "refactor",
							"section": "ğŸ’… Refactors"
						},
						{
							"type": "docs",
							"section": "ğŸ“– Documentation"
						},
						{
							"type": "build",
							"section": "ğŸ“¦ Build"
						},
						{
							"type": "types",
							"section": "ğŸŒŠ Types"
						},
						{
							"type": "chore",
							"section": "ğŸ¡ Chore"
						},
						{
							"type": "examples",
							"section": "ğŸ€ Examples"
						},
						{
							"type": "test",
							"section": "âœ… Tests"
						},
						{
							"type": "style",
							"section": "ğŸ¨ Styles"
						},
						{
							"type": "ci",
							"section": "ğŸ¤– CI"
						}
					]
				}
			}
		],
		[
			"@semantic-release/changelog",
			{
				"changelogFile": "docs/CHANGELOG.md"
			}
		],
		[
			"@semantic-release/npm",
			{
				"npmPublish": false
			}
		],
		[
			"@semantic-release/git",
			{
				"message": "chore: release v${nextRelease.version}\n\n",
				"assets": [
					"package.json",
					"docs/CHANGELOG.md"
				]
			}
		],
		"@semantic-release/github"
	]
}
